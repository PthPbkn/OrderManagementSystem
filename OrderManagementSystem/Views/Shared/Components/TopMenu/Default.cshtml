@model IEnumerable<Menu>

<ul class="navbar-nav flex-grow-1">
    @foreach(var item in Model)
    {
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="@item.Area" asp-controller="@item.Controller" asp-action="@item.Action">@item.DisplayName</a>
        </li>
    }   
    
</ul>